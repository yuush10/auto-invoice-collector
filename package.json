{
  "name": "auto-invoice-collector",
  "version": "0.1.0",
  "description": "Automatically collect invoices and receipts from Gmail to Google Drive",
  "type": "module",
  "main": "dist/bundle.js",
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "push": "npm run build && clasp push",
    "watch": "rollup -c rollup.config.mjs -w",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "login": "clasp login",
    "logout": "clasp logout",
    "open": "clasp open-script",
    "logs": "clasp tail-logs",
    "deploy": "npm run build && clasp push && clasp create-deployment"
  },
  "keywords": [
    "google-apps-script",
    "invoice",
    "automation",
    "gmail",
    "drive"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/google-apps-script": "^1.0.83",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "rollup": "^4.30.1",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {}
}
